      "SystemCapability.HiviewDFX.HiLog"
    ],
    "adapted_system_type": [
      "standard"
    ],
    "rom": "188KB",
    "ram": "2000KB",
    "deps": {
      "components": [
        "c_utils",
        "init",
        "napi"
      ],
